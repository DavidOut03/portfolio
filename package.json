{
  "name": "portfolio-v3",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",

    "watch:sass": "node-sass sass/main.scss src/css/main.css -w",
    "start": "npm-run-all --parallel dev watch:sass",
    "compile:sass": "node-sass sass/main.scss src/css/main.comp.css",
    "concat:css": "concat -o css/main.concat.css src/css/main.comp.css",
    "prefix:css": "postcss --use autoprefixer -b 'last 8 versions' src/css/main.concat.css -o src/css/main.prefix.css",
    "compress:css": "node-sass src/css/main.prefix.css src/css/main.css --output-style compressed",
    "build:css": "npm-run-all compile:sass concat:css prefix:css compress:css"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "autoprefixer": "^10.4.7",
    "concat": "^1.0.3",
    "gh-pages": "^5.0.0",
    "live-server": "^1.2.2",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0"
  }
}
